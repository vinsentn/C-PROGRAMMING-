// assignmemt operators;
#include <stdio.h>

int main()
{

    int a =5, b = 5;


    printf("a=b  : %d\n", a=b);
    printf("a+=b  : %d\n", a+=b);
    printf("a -= b: %d\n", a -= b);
    printf("a *= b: %d\n", a *= b);
    printf("a /= b: %d\n", a /= b);
    printf("a %= b : %d\n", a%= b);

    return 0;
}
output;
a=b  : 5
a+=b  : 10
a -= b: 5
a *= b: 25
a /= b: 5
a %= b : 0


=== Code Execution Successful ===
