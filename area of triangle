#include <stdio.h>

int main() 
{
float a,b,area;
    printf("enter the value of base=");
    scanf("%f",&a);
    printf("enter the value of height=");
    scanf("%f",&b);
    area=0.5*(a*b);
      printf("area of triangle=%f\n",area);
    
    return 0;
}
