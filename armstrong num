#include <stdio.h>
int main()
{
int num=153,originalnum,rem,result=0;
originalnum=num;
while(originalnum!=0)
{
    rem=originalnum%10;
    result+=rem*rem*rem;
    originalnum/=10;
}
if(result==num)
printf("%d is an armstrong number",num);
else
printf("%d is not an armstrong number",num);


    return 0;
}
output
153 is an armstrong number

=== Code Execution Successful ===
